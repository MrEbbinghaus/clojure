@top[name=Program] { expression* }

@skip { whitespace }
expression { Symbol | List }

List { "(" (DefLike VarName expression+ | expression*) ")" }
VarName { Symbol }

@tokens {
  "("
  ")"
  whitespace { std.whitespace }
  Symbol { std.asciiLetter+ }
}

DefLike { @extend<Symbol, "def" | "defn"> }

@detectDelim
